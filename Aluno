class Aluno:
    def __init__(self, nome, matricula):
        self.nome = nome
        self.matricula = matricula
    def get_nome(self):
      return self.nome

    def set_nome(self, nome):
      self.nome = nome

    def get_matricula(self):
      return self.matricula

    def set_matricula(self, matricula):
      self.matricula = matricula


aluno = Aluno("Andressa", "12345")
print(aluno.get_nome())
